{
  "name": "editorial-tools-integration-tests",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "./scripts/setup.sh $ENV && npm run cy:run-dev",
    "grid-prod": "APP=grid STAGE=prod npm run doit",
    "grid-code": "APP=grid STAGE=test npm run doit",
    "grid-test": "APP=grid STAGE=test npm run doit",
    "grid-local": "APP=grid STAGE=local npm run doit",
    "composer-prod": "APP=composer STAGE=prod npm run doit",
    "composer-code": "APP=composer STAGE=code npm run doit",
    "composer-local": "APP=composer STAGE=local npm run doit",
    "doit": "node ./src/utils/cookie.js && cypress run --reporter spec --env APP=$APP,STAGE=$STAGE --spec cypress/integration/$APP/*",
    "cy:live": "node ./src/utils/cookie.js && cypress run --env APP=$APP,STAGE=$STAGE  --spec cypress/integration/$APP/*",
    "cy:run-dev": "cypress run --reporter spec",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "lint": "eslint cypress/integration/* src/* reporters",
    "riffraff": "node-riffraff-artifact",
    "upload-video": "node scripts/uploadVideo.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@guardian/node-riffraff-artifact": "^0.1.8",
    "@guardian/pan-domain-node": "^0.4.2",
    "@types/node-fetch": "^2.5.6",
    "cypress": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "mocha": "^7.1.1"
  },
  "dependencies": {
    "aws-sdk": "^2.656.0",
    "iniparser": "^1.0.5",
    "node-fetch": "^2.6.0"
  }
